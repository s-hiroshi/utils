{
    "name": "s-hiroshi/utils",
    "license": "MIT",
    "description": "Utility command script.",
    "require": {
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
      "psr-4": {
        "SH\\Utils\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SH\\Utils\\": [
          "tests/",
          "tests/Fake"
        ]
      }
    },
    "scripts": {
      "test": [
        "./vendor/bin/phpunit"
      ],
      "build": [
        "rm -rf ./dist",
        "mkdir -p ./dist",
        "php -d phar.readonly=0 ./bin/box.phar build"
      ]
    },
    "authors": [
        {
            "name": "Hiroshi Sawai",
            "email": "info@info-town.jp"
        }
    ]
}
